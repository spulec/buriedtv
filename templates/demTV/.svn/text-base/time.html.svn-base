{% extends "base.html" %}
{% load facebook_tags %}

{% block title %}All days{% endblock %}

{% block content %}

Days:
{% if days %}
    <ul>
    {% for day in days %}
        <li><a href="{% url demTvDjango.demTV.views.day day.pk %}">{{ day.name }}</a></li>
    {% endfor %}
    </ul>
{% else %}
    <p>Error getting days.</p>
{% endif %}

{% endblock %}
